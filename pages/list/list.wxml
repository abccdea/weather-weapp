<view class='date-item' wx:for="{{weekWeather}}" wx:key="day">
  <view class="date-wrapper">
    <view class="day">{{item.day}}</view>
    <view class="date">{{item.date}}</view>
  </view>
  <view class="temp">{{item.temp}}</view>
  <image class="weather-icon" src="/images/sunny-icon.png"></image>
</view>